{
    "parser":"babel-eslint",
    "rules":{
        "import/no-unresolved": [2, {"commonjs": true,"amd": true}],
        "import/named": 2,
        "import/namespace": 2,
        "import/default": 2,
        "import/export": 2
    },
    "extends": ["plugin:import/warnings"] ,
    "plugins": ["import"]
}